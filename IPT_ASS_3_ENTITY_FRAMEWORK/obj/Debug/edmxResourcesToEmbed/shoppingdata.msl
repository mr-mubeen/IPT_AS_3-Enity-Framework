      </EntityTypeMapping>
    </EntitySetMapping>
    <EntitySetMapping Name="orders">
      <EntityTypeMapping TypeName="ShoppingModel.order">
        <MappingFragment StoreEntitySet="orders">
          <ScalarProperty Name="o_id" ColumnName="o_id" />
