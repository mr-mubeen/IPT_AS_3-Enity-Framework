          <ScalarProperty Name="p_id" ColumnName="p_id" />
          <ScalarProperty Name="c_id" ColumnName="c_id" />
        </MappingFragment>
      </EntityTypeMapping>
    </EntitySetMapping>
    <EntitySetMapping Name="products">
      <EntityTypeMapping TypeName="ShoppingModel.product">
        <MappingFragment StoreEntitySet="products">
          <ScalarProperty Name="p_id" ColumnName="p_id" />
